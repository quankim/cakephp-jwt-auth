{
    "name": "quankim/cakephp-jwt-auth",
    "description": "QuanKim/JwtAuth plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "quankim/php-jwt":"*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "QuanKim\\JwtAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QuanKim\\JwtAuth\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
